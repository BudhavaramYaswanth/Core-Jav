package app1;

public class Calculator 
{
	
	private int age = 30;
	
	public static void main(String[] args) {
		
		Calculator c = new Calculator();
		
		c.add(20, 50);
	}
	
	private int add(int i, int j)
	{
		int k = i+j;
		return k;
	}
	
	public int sub(int i, int j)
	{
		int k = i-j;
		return k;
	}
	
	public int mul(int i, int j)
	{
		int k = i*j;
		return k;
	}
	
	public int div(int i, int j)
	{
		int k = i/j;
		return k;
	}
}
